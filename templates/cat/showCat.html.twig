{% extends 'base.html.twig' %}

{% block title %}
	Page d'accueil
{% endblock %}

{% block body %}
	<div class="container">
		<h1 class="mt-4">
			Nom :
			{{ cat.name }}</h1>

		<p>Race :
			{{ cat.race }}</p>
		<p>Age :
			{{ cat.age }}</p>
		<p>Couleur :
			{{ cat.color }}</p>
		<p>Localisation :
			{{ cat.localisation }}</p>
		<p>Photo :
			{{ cat.picture }}</p>
	</div>
{% endblock %}
